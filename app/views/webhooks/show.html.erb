<div class="row" id="request_detail">
  <div class="col-md-3">
    <%= render "backpacks/left_list", backpacks: @backpacks, current_backpack: @current_backpack %>
  </div>
  <div class="col-md-9">


<!-- Modal -->
<%= render 'webhook_response_settings' %>
<%= render 'webhooks/script_edit' %>

    <%= render "header" %>
    <% if @current_backpack.present? %>
      <div class=""></div>
      <div class="row">
        <div class="col-md-5">
          <%= render "backpacks/request_detail", backpack: @current_backpack %>
          <%= render "backpacks/request_query", backpack: @current_backpack %>
          <%= render "backpacks/request_form", backpack: @current_backpack %>
          <%= render "backpacks/request_file", backpack: @current_backpack %>
          <% if !['application/x-www-form-urlencoded', 'multipart/form-data'].include?(@current_backpack.content_type) %>
            <%= render "backpacks/request_raw_content", backpack: @current_backpack %>
          <% end %>
        </div>

        <div class="col-md-7">
          <%= render "backpacks/request_headers", backpack: @current_backpack %>
        </div>
      </div>
    <% else %>
      <div class="row">
        <div class="col-md-5">
          <div class="card">
            <div class="card-header">
              暂时还没有请求
            </div>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>

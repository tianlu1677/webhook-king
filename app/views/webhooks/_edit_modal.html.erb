<div class="modal fade" id="WebhookEditModal" tabindex="-1" aria-labelledby="WebhookEdit" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="WebhookResponseSettingModalLabel">Update webhook</h5>
        <button type="button" class="btn-close"  data-coreui-dismiss="modal"></button>
      </div>
        <%= form_with model: @webhook, url: "/webhooks/#{@webhook.uuid}", method: :patch, local: true, html: {"data-turbo" => false} do |f| %>
      <div class="modal-body">
          <div class='mb-12 row'>            
            <%= f.label :short %>
            <%= f.text_field :short, class: 'form-control'%>
          </div>          
      </div>
      <div class="modal-footer">
        <%= f.submit "Save", class: 'btn btn-primary' %>
      </div>
       <%end %>
    </div>
  </div>
</div>
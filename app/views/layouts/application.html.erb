<!DOCTYPE html>
<html>
<head>
  <title>Webhook</title>
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>
  <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  <%= stylesheet_pack_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>

</head>
<body class="app header-fixed sidebar-fixed aside-menu-fixed sidebar-lg-show  page-<%= controller_name %>">
  <div class="c-wrapper c-fixed-components">
    <%#= render "shared/header" %>
    <div class="c-body">
      <main class="c-main">
        <div class="container-fluid">
          <div class="fade-in">
            <%= yield %>
          </div>
        </div>
      </main>
    </div>
  </div>
</body>
</html>

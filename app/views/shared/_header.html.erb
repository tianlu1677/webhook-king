<header class="header header-sticky mb-4">
  <div class="container-fluid">
    <button class="header-toggler px-md-0 me-md-3 d-xxl-none" type="button" onclick="coreui.Sidebar.getInstance(document.querySelector('#sidebar')).toggle()">
      <svg class="icon icon-lg">
        <use xlink:href="/coreui/icons/brand/coreui.svg#full"></use>
      </svg>
    </button>
    <a class="header-brand d-xl-none" href="#">
      <svg width="118" height="46" alt="CoreUI Logo">
        <use xlink:href="/coreui/icons/brand/coreui.svg#full"></use>
      </svg>
    </a>
    <ul class="header-nav d-md-down-none">
      <li class="nav-item"><a class="nav-link" href="/">Home</a></li>
      <li class="nav-item"><a class="nav-link" href="https://webhook.site">Webhook.site</a></li>
    </ul>
    <ul class="header-nav ms-auto">

    </ul>

    <ul class="header-nav ms-3">
      <% if signed_in? %>
        <li class="nav-item dropdown d-flex align-items-center">
          <a class="nav-link py-0" data-coreui-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">
            <div class="avatar avatar-md"><img class="avatar-img" src="/avatars/8.jpg" alt="user@email.com"></div>
          </a>
          <div class="dropdown-menu dropdown-menu-end pt-0">
            <div class="dropdown-header bg-light py-2">
              <div class="fw-semibold">Account</div>
            </div>
            <%= link_to session_path, method: :delete do %>
              <div class="dropdown-item">
                Logout
            <% end %>
            </div>

          </div>
        </li>
      <% else %>
        <li>
          <%= link_to '登陆', new_session_path, class: 'btn btn-primary btn-sm' %>
        </li>
      <% end %>
    </ul>
  </div>
  <div class="header-divider"></div>
<!--  <div class="container-fluid">-->
<!--    <nav aria-label="breadcrumb">-->
<!--      <ol class="breadcrumb my-0 ms-2">-->
<!--        <li class="breadcrumb-item">-->
<!--          <span>Home</span>-->
<!--        </li>-->
<!--        <li class="breadcrumb-item active"><span>Dashboard</span></li>-->
<!--      </ol>-->
<!--    </nav>-->
<!--  </div>-->
</header>
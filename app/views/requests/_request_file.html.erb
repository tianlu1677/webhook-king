<div class="card">
  <div class="card-header"><i class="fa fa-align-justify"></i> File Params</div>
  <div class="card-body">
    <table class="table table-responsive-sm">
      <tbody>
      <% (request.files || []).each do |file| %>
        <tr>
          <td><%= file.blob.metadata['params_key'] %></td>
          <td><%#= link_to "11",Rails.application.routes.url_helpers.rails_blob_path(file) %></td>
        </tr>
      <% end %>
      </tbody>
    </table>
  </div>
</div>